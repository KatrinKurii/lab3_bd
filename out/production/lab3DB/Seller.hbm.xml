<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://hibernate.sourceforge.net/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class
            name="database.entry.Seller"
            table="seller">
        <id
                name="sid">
            <generator class="increment"/>
        </id>
        <property name="sname"/>
    </class>
</hibernate-mapping>